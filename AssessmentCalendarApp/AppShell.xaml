<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="AssessmentCalendarApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:AssessmentCalendarApp.Pages"
    xmlns:local="clr-namespace:AssessmentCalendarApp"
    Shell.FlyoutBehavior="Flyout"
    Title="AssessmentCalendarApp">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Assessment Test"
        ContentTemplate="{DataTemplate pages:assessmenttest}"
        Route="assessmenttest" />
</Shell>
