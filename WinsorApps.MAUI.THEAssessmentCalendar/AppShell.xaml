<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="WinsorApps.MAUI.THEAssessmentCalendar.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
    xmlns:pages="clr-namespace:WinsorApps.MAUI.THEAssessmentCalendar.Pages"
    xmlns:local="clr-namespace:WinsorApps.MAUI.THEAssessmentCalendar"
    Shell.FlyoutBehavior="Flyout"
    Title="WinsorApps.MAUI.THEAssessmentCalendar">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    
    <ShellContent
        Title="Test"
        ContentTemplate="{DataTemplate pages:assessmenttest}"
        Route="assessmenttest" />
    <ShellContent
        Title="Calendar"
        ContentTemplate="{DataTemplate pages:Calendar_Page}"
        Route="Calendar_Page" />

</Shell>
